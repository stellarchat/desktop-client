{
  "name": "foxlet",
  "version": "5.0.0",
  "productName": "Foxlet",
  "description": "Stellar desktop client",
  "main": "main.js",
  "homepage": "https://wallet.stellar.chat/",
  "repository": {
    "type": "git",
    "url": "https://github.com/stellarchat/desktop-client.git"
  },
  "author": {
    "name": "stellarchat",
    "email": "stellar@ripplefox.com"
  },
  "license": "MIT",
  "keywords": [
    "stellar",
    "wallet",
    "electron",
    "angularjs"
  ],
  "scripts": {
    "start": "electron .",
    "build": "concurrently --kill-others-on-fail -n LINUX,MAC__,WIN__ 'yarn run build:linux' 'yarn run build:mac' 'yarn run build:win'",
    "build:linux": "electron-builder --x64 --linux",
    "build:mac": "electron-builder --x64 --mac",
    "build:win": "electron-builder --x64 --win",
    "lint": "eslint . --ext=js,html",
    "lintfix": "eslint . --ext=js,html --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "angular": "^1.7.2",
    "angular-chart.js": "^1.1.1",
    "angular-qrcode": "^7.2.0",
    "angular-route": "^1.7.2",
    "angular-translate": "^2.18.1",
    "bootstrap": "^3.3.7",
    "chart.js": "^2.7.2",
    "electron-notifications": "^1.0.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "qrcode-generator": "^1.4.0",
    "sjcl": "^1.0.7",
    "stellar-sdk": "^0.8.2",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "devtron": "^1.4.0",
    "electron": "2.0.2",
    "electron-builder": "20.15.1",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^12.1.0",
    "eslint": "^4.19.1",
    "eslint-plugin-html": "^4.0.3"
  }
}
